<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="339.0" prefWidth="542.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.LoginController">
   <children>
      <Button fx:id="signInButton" disable="true" layoutX="238.0" layoutY="228.0" mnemonicParsing="false" onMouseClicked="#onSignInClicked" prefHeight="25.0" prefWidth="67.0" text="Sign In" />
      <Button fx:id="newUserButton" layoutX="238.0" layoutY="276.0" mnemonicParsing="false" onMouseClicked="#onNewUserClicked" text="New User" />
      <Label layoutX="113.0" layoutY="107.0" text="Username:" />
      <Label layoutX="113.0" layoutY="170.0" text="Password:" />
      <TextField fx:id="enterUsername" layoutX="197.0" layoutY="103.0" onKeyReleased="#onUsernameChanged" promptText="Enter Username..." />
      <TextField fx:id="enterPassword" layoutX="197.0" layoutY="166.0" onKeyReleased="#onPasswordChanged" promptText="Enter Password..." />
      <Label fx:id="invalidLabel" layoutX="370.0" layoutY="99.0" prefHeight="87.0" prefWidth="149.0" />
   </children>
</AnchorPane>
