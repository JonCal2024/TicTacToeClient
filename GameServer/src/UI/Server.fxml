        <ListView fx:id="inactiveGamesList" layoutX="318.0" layoutY="400.0" onMouseClicked="#onCompletedGamesListClicked" prefHeight="222.0" prefWidth="268.0" />
        <Label layoutX="403.0" layoutY="383.0" text="Completed Games" />
        <Button fx:id="inactiveGameDetailsButton" layoutX="376.0" layoutY="663.0" mnemonicParsing="false" onMouseClicked="#onInactiveGameDetailsClicked" prefHeight="25.0" prefWidth="152.0" text="Completed Game Details" />
