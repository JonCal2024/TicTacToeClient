      <Label layoutX="116.0" layoutY="54.0" text="Registered Players" />
      <Label fx:id="errorActiveLabel" alignment="CENTER" layoutX="29.0" layoutY="622.0" prefHeight="32.0" prefWidth="267.0" style="-fx-background-color: #ffffff; -fx-border-color: gray;" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="errorCompletedLabel" alignment="CENTER" layoutX="319.0" layoutY="622.0" prefHeight="32.0" prefWidth="267.0" style="-fx-background-color: white; -fx-border-color: grey;" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="errorModifyLabel" alignment="CENTER" layoutX="31.0" layoutY="293.0" prefHeight="32.0" prefWidth="267.0" style="-fx-background-color: white; -fx-border-color: grey;" textFill="RED">
